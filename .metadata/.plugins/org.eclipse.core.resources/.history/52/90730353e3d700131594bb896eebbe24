// Package
package RASA;

public class Time_Node {

	public int Time;
	public Internal_Clock Parent_IC;
	
	// List
	public Time_Node Next;
	public Time_Node Prev;
	
	public Time_Node(int Creation_Time, Internal_Clock IC) {
		// TODO Auto-generated constructor stub
		Time = Creation_Time;
		Next = null;
		Prev = null;
		Parent_IC = IC;
	}

}
